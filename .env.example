# UrbanAI Environment Variables
# Скопируйте этот файл как .env и заполните значения

# Django Secret Key (обязательно сменить в продакшене!)
SECRET_KEY=your-super-secret-key-change-me-in-production-use-long-random-string

# Разрешенные хосты (через запятую)
ALLOWED_HOSTS=localhost,127.0.0.1,your-domain.com,your-server-ip

# Debug mode (False для продакшена)
DEBUG=False

# Google Gemini API Key
GEMINI_API_KEY=your-gemini-api-key-here
